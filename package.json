    "url": "https://github.com/cleanRank/sool-template/issues"
  },
  "homepage": "https://github.com/cleanRank/sool-template#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-env": "^2.4.1",
    "chalk": "^3.0.0",
    "commander": "^5.0.0",
    "download-git-repo": "^3.0.2",
    "ini": "^1.3.5",
    "inquirer": "^7.1.0",
    "log-symbols": "^3.0.0",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0"
  }
